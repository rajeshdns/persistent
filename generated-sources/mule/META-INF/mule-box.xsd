<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:ns2="http://www.mulesoft.org/schema/mule/devkit" xmlns:mule="http://www.mulesoft.org/schema/mule/core" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://www.mulesoft.org/schema/mule/box">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"></xs:import>
  <xs:import schemaLocation="http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" namespace="http://www.springframework.org/schema/beans"></xs:import>
  <xs:import schemaLocation="http://www.mulesoft.org/schema/mule/core/current/mule.xsd" namespace="http://www.mulesoft.org/schema/mule/core"></xs:import>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="config" substitutionGroup="mule:abstract-extension" ns2:javaClass="org.mule.modules.box.adapters.BoxConnectorOAuth2Adapter">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation>Box Cloud Connector for API V2.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element name="oauth-callback-config" maxOccurs="1" minOccurs="1">
              <xs:annotation>
                <xs:documentation>Config for http callbacks.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute name="localPort" use="optional" type="xs:string"></xs:attribute>
                    <xs:attribute name="remotePort" use="optional" type="xs:string"></xs:attribute>
                    <xs:attribute name="domain" use="optional" type="xs:string"></xs:attribute>
                    <xs:attribute name="async" default="false" use="optional" type="xs:boolean"></xs:attribute>
                    <xs:attribute name="path" use="optional" type="xs:string"></xs:attribute>
                    <xs:attribute name="connector-ref" use="optional" type="xs:string"></xs:attribute>
                    <xs:attribute name="defaultAccessTokenId" use="optional" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>A Mule Expression to use as access token id</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="oauth-store-config" maxOccurs="1" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Configuration element for storage of access tokens</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute name="objectStore-ref" use="required" type="xs:string"></xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="name" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>Give a name to this configuration so it can be later referenced by config-ref.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="baseUrl" default="https://api.box.com/2.0/" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>The api's base url</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="uploadUrl" default="https://upload.box.com/api/2.0/files" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>The url of the endpoints dedicated to file uploading operations</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="clientId" use="required" type="xs:string">
            <xs:annotation>
              <xs:documentation>The OAuth2 client id</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="clientSecret" use="required" type="xs:string">
            <xs:annotation>
              <xs:documentation>The OAuth2 client secret</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="useGzip" default="false" use="optional" type="booleanType">
            <xs:annotation>
              <xs:documentation>If set to true, Box will be asked to gzip all its responses</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="accessTokenUrl" default="https://www.box.com/api/oauth2/token" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="onNoToken" default="EXCEPTION" use="optional" type="OnNoTokenPolicyEnumType">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="authorizationUrl" default="https://www.box.com/api/oauth2/authorize" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="authorize" substitutionGroup="mule:abstract-message-processor" type="AuthorizeType">
    <xs:annotation>
      <xs:documentation>Starts OAuth authorization process. It must be called from a flow with an http:inbound-endpoint.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="unauthorize" substitutionGroup="mule:abstract-message-processor" type="UnauthorizeType">
    <xs:annotation>
      <xs:documentation>Unauthorizes the connector, forcing to re-use authorize again before accessing any protected message processor.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AuthorizeType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="state" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Any value that you wish to be sent with the callback</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenUrl" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The Id with which the obtained access token will be stored. If not provided, then it will be the config name</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="authorizationUrl" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UnauthorizeType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="get-folder" substitutionGroup="mule:abstract-message-processor" type="GetFolderType">
    <xs:annotation>
      <xs:documentation>Retrieves information about a given folder. If the folderId parameter is
not provided or equals 0, then the root folder will be returned.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="GetFolderType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="folderId" default="0" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>the id of the folder you want to get. 0 means root</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="getAllAttributes" default="false" use="optional" type="booleanType">
          <xs:annotation>
            <xs:documentation>option to retrieve all folder attributes or just the default ones</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="create-folder" substitutionGroup="mule:abstract-message-processor" type="CreateFolderType">
    <xs:annotation>
      <xs:documentation>Creates a new folder and returns a folder object with all its associated
information</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateFolderType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="parentId" default="0" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>the id of the parent folder. If not provided then the root will be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="folderName" use="required" type="xs:string">
          <xs:annotation>
            <xs:documentation>the name of the folder</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="update-folder" substitutionGroup="mule:abstract-message-processor" type="UpdateFolderType">
    <xs:annotation>
      <xs:documentation>Used to update information about the folder. To move a folder, update the
ID of its parent. To enable an email address that can be used to upload
files to this folder, update the folderUploadEmail attribute. An optional
If-Match header can be included to ensure that client only updates the
folder if it knows about the latest version by setting the etag
attribute.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="EntityObjectType">
    <xs:sequence></xs:sequence>
    <xs:attribute name="type" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>string representing the item's type</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="id" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>the id of the item</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The name of the item</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="etag" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>A unique string identifying the version of this folder</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sequenceId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>A unique ID for use with the /events endpoint</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ref" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="ItemObjectType">
    <xs:complexContent>
      <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="EntityObjectType">
        <xs:sequence></xs:sequence>
        <xs:attribute name="sha1" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The sha1 hash of this item</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PermissionsObjectType">
    <xs:sequence></xs:sequence>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="preview" use="optional" type="booleanType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="download" use="optional" type="booleanType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ref" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="SharedLinkObjectType">
    <xs:sequence>
      <xs:element name="permissions" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="PermissionsObjectType"></xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="access" use="optional" type="AccessEnumType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="passwordEnabled" use="optional" type="booleanType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="url" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="downloadUrl" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="vanityUrl" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="previewCount" use="optional" type="longType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="downloadCount" use="optional" type="longType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="unsharedAt" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ref" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="FolderUploadEmailObjectType">
    <xs:sequence></xs:sequence>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="access" use="optional" type="AccessEnumType">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="email" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ref" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="UpdateItemRequestObjectType">
    <xs:sequence>
      <xs:element name="parent" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="ItemObjectType"></xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="shared-link" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="SharedLinkObjectType"></xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="permissions" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="PermissionsObjectType"></xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="folder-upload-email" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="FolderUploadEmailObjectType"></xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="tags" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="tag" maxOccurs="unbounded" minOccurs="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="value-ref" use="optional" type="xs:string"></xs:attribute>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="ref" use="optional" type="xs:string">
            <xs:annotation>
              <xs:documentation>The reference object for this parameter</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="name" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The name of the folder</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="description" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The description of the folder</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="parentId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The ID of the parent folder</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="access" use="optional" type="AccessEnumType">
      <xs:annotation>
        <xs:documentation>The level of access required for this shared link. Can be open, company, collaborators</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="unsharedAt" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The day that this link should be disabled at. Timestamps are rounded off to the given day.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="canDownload" use="optional" type="booleanType">
      <xs:annotation>
        <xs:documentation>Whether this link allows downloads</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/box" name="canPreview" use="optional" type="booleanType">
      <xs:annotation>
        <xs:documentation>Whether this link allows previews</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ref" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="UpdateFolderType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:sequence>
          <xs:element name="request" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>an instance of  with the attributes you want to change</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>
                <xs:extension xmlns="http://www.mulesoft.org/schema/mule/box" base="UpdateItemRequestObjectType"></xs:extension>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="folderId" use="required" type="xs:string">
          <xs:annotation>
            <xs:documentation>the id of the folder to be modified</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="etag" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>if provided, it will be used to verify that no newer version of the file is available at box</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/box" xmlns:mule="http://www.mulesoft.org/schema/mule/core" name="get-folder-discussions" substitutionGroup="mule:abstract-message-processor" type="GetFolderDiscussionsType">
    <xs:annotation>
      <xs:documentation>Retrieves the discussions on a particular folder, if any exist.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="GetFolderDiscussionsType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute name="config-ref" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="folderId" use="required" type="xs:string">
          <xs:annotation>
            <xs:documentation>the id of the folder which discussions you want</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accessTokenId" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
